<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>DawgTrades</display-name>
  <welcome-file-list>
    <welcome-file>Login.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>edu.uga.dawgtrades.boundary.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AuctionItem</servlet-name>
    <servlet-class>edu.uga.dawgtrades.boundary.AuctionItem</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AuctionItem</servlet-name>
    <url-pattern>/AuctionItem</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AuctionItemResult</servlet-name>
    <servlet-class>edu.uga.dawgtrades.boundary.AuctionItemResult</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AuctionItemResult</servlet-name>
    <url-pattern>/AuctionItemResult</url-pattern>
  </servlet-mapping>
</web-app>